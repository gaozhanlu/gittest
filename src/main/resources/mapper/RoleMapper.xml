<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gzl.model.dao.IRoleDao">
    <select id="getRoleById" parameterType="java.lang.String" resultType="com.gzl.model.entity.RoleEntity">
